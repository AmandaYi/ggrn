// 作物种类模态窗状态
// 展示模态窗
export const SHOW_MODAL_CROP_SPEC="SHOW_MODAL_CROP_SPEC"
// 隐藏模态窗
export const HIDE_MODAL_CROP_SPEC="HIDE_MODAL_CROP_SPEC"
//  展示模态窗
export  function showModalCropSpec (visable){
    return {
        type:SHOW_MODAL_CROP_SPEC,
        payload:{
            visable
        }
    }
}
// 隐藏模态窗
export function hideModalCropSpec (visable){
    return {
        type:SHOW_MODAL_CROP_SPEC,
        payload:{
            visable
        }
    }
}
 